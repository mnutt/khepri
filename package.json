{
  "name": "khepri",
  "productName": "khepri",
  "version": "0.5.1",
  "description": "Process manager gui",
  "repository": "https://github.com/mnutt/khepri",
  "license": "MIT",
  "author": "Michael Nutt <michael@nutt.im>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "private": false,
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "electron-forge start",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "dependencies": {
    "@electron-forge/cli": "6.0.0-beta.51",
    "ansi_up": "^3.0.0",
    "daemon": "^1.1.0",
    "debug": "^4.1.1",
    "rsvp": "^4.8.5",
    "tail": "^2.0.4",
    "user-env": "^2.0.0",
    "xterm": "^4.8.1",
    "xterm-addon-fit": "^0.4.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@ember/render-modifiers": "^1.0.2",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "~3.19.0",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-babel": "^7.20.5",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^5.1.2",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-electron": "3.0.0-beta.3",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.19.0",
    "ember-template-lint": "^2.8.0",
    "eslint": "^7.1.0",
    "eslint-plugin-ember": "^8.6.0",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.2.0"
  },
  "main": "ember-electron/main.js",
  "ember-electron": {
    "copy-files": [
      "electron",
      "package.json",
      "dist",
      "public"
    ],
    "name": "Khepri",
    "version": "1.4.0",
    "app-bundle-id": "im.nutt.khepri",
    "app-category-type": "public.app-category.developer-tools",
    "icon": "public/assets/images/icon.icns"
  },
  "volta": {
    "node": "14.5.0"
  }
}
